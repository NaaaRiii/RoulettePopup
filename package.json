{
  "name": "roulettepopup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start -p 4000",
    "lint": "next lint",
    "test": "jest --verbose",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/roulette_spin.cy.js'",
    "test:e2e:debug": "cypress run --spec 'cypress/e2e/roulette_spin_debug.cy.js'",
    "test:e2e:prod": "cypress run --spec 'cypress/e2e/roulette_spin_production.cy.js'",
    "test:e2e:prod:open": "cypress open --config-file cypress.config.ts",
    "test:e2e:amplify": "cypress run --spec 'cypress/e2e/roulette_spin_amplify.cy.js'",
    "test:e2e:amplify:debug": "cypress run --spec 'cypress/e2e/roulette_spin_amplify_debug.cy.js'",
    "test:e2e:amplify:simple": "cypress run --spec 'cypress/e2e/roulette_spin_amplify_simple.cy.js'",
    "test:e2e:amplify:prod": "cypress run --config-file cypress.config.prod.ts --spec 'cypress/e2e/amplify_auth_test.cy.js'",
    "test:e2e:amplify:prod:open": "cypress open --config-file cypress.config.prod.ts",
    "test:e2e:amplify:prod:fixed": "cypress run --config-file cypress.config.prod.ts --spec 'cypress/e2e/amplify_auth_test.cy.js' --env failOnStatusCode=false",
    "cypress:setup": "cp cypress.env.example.json cypress.env.json && echo 'cypress.env.json を作成しました。必要に応じて編集してください.'"
  },
  "dependencies": {
    "@aws-amplify/auth": "^6.8.2",
    "@aws-amplify/ui-react": "^6.9.3",
    "aws-amplify": "^6.13.5",
    "axios": "^1.6.2",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.0.1",
    "next": "14.0.3",
    "next-auth": "^4.24.10",
    "react": "^18",
    "react-calendar": "^4.8.0",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.21.2",
    "recharts": "^2.11.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.9.0",
    "@aws-amplify/backend-cli": "^1.4.3",
    "@mermaid-js/mermaid-cli": "^11.6.0",
    "@swc/jest": "^0.2.38",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "aws-cdk": "^2.173.0",
    "aws-cdk-lib": "^2.173.0",
    "constructs": "^10.4.2",
    "cypress": "^14.5.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.24.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^16.0.0",
    "json-schema-to-ts": "^3.1.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-algebra": "^2.0.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  }
